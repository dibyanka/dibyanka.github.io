Declare @DatePart varchar(23),
Declare @TimePart varchar(23),
Declare @Output DateTime

SELECT @Date='2019-01-14 00:00:00.000'
      ,@Time='11:11' -- it can be used for something like '2019-01-14 09:40:12.523'
      
      SELECT @Output=LEFT(@DatePart,10)+' '+RIGHT(@TimePart,12)
      SELECT @Output --2019-01-14 11:11:00.000
